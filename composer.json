{
    "name": "nayjest/db-diff",
    "description": "Database diff tool for Laravel & MySQL",
    "keywords": ["laravel", "db", "diff", "database", "mysql"],
    "authors": [
        {
            "name": "Vitalii [Nayjest] Stepanenko",
            "email": "v.stepanenko@globo-tech.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "nayjest/grids": ">=0.6",
        "illuminate/support": "4.2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Nayjest/Grids.git"
        }
    ],
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Nayjest\\DbDiff\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
